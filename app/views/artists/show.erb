<h1><%= @artist.name %></h1>
<br>
<h2>Songs</h2>
<% @artist.songs.each do |song| %>
  <a id="artist_name" href="/songs/<%=song.slug%>"><br><%=song.name%></a>
<% end %>
<br>
<br>
<h2>Genres</h2>
<% @artist.genres.each do |genre| %>
  <a href="/genres/<%=genre.slug%>"><br><%=genre.name%></a>
<% end %>
